{
  "site_name": "dstz (Dempster-Shafer Theory Â· ztxtech) PyPi package",
  "site_description": "Documentation for the project",
  "repo_url": "https://github.com/ztxtech/dstz",
  "repo_name": "ztxtech/dstz",
  "theme": {
    "name": "material",
    "palette": {
      "scheme": "default",
      "primary": "indigo",
      "accent": "indigo"
    },
    "font": {
      "text": "Roboto",
      "code": "Roboto Mono"
    },
    "features": [
      "navigation.tabs",
      "navigation.sections",
      "toc.integrate",
      "navigation.top",
      "search.suggest",
      "search.highlight",
      "content.tabs.link"
    ]
  },
  "docs_requirements": [
    "mkdocs",
    "mkdocs-material",
    "mkdocstrings[python]",
    "pyyaml"
  ]
}